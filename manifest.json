{
    "name": "MusicBlocks",
    "version": "1.1",
    "manifest_version": 2,
    "author": "Sugar Labs",
    "description": "Learn to program music with snap-together blocks.",
    "icons": [
        {  
            "src": "/activity/activity-icon-color-512.png",  
            "sizes": "512x512",  
            "type": "image/png"  
        }, 
        {  
            "src": "/activity/activity-icon-color-128.png",  
            "sizes": "128x128",  
            "type": "image/png"  
        }
    ],
    "app": {
	"background": {
	    "scripts": ["js/launch.js"]
	}
    },
    "permissions": [
	"storage",
	"unlimitedStorage",
	"audioCapture",
	"videoCapture"
    ]
    "background_color": "#92B5C8",
    "theme_color": "#92B5C8"
}
